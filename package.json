{
  "name": "curve",
  "version": "1.0.0",
  "description": "Mathematical curve drawing app",
  "author": "Labreche Abdellatif",
  "main": "./dist/app.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "rebuild": "electron-rebuild -f -w sqlite3",
    "electron": "electron ./dist/app.js",
    "dist": "build",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "appId": "com.curve.app",
    "directories": {
      "buildResources": "dist/build",
      "output": "dist/build"
    },
    "files": [
      "./dist/**/*",
      "./dist/app.js"
    ],
    "win": {
      "target": "NSIS",
      "icon": "build/icon.ico"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.2",
    "@angular/common": "^6.0.2",
    "@angular/compiler": "^6.0.2",
    "@angular/core": "^6.0.2",
    "@angular/elements": "^7.2.7",
    "@angular/forms": "^6.0.2",
    "@angular/http": "^6.0.2",
    "@angular/platform-browser": "^6.0.2",
    "@angular/platform-browser-dynamic": "^6.0.2",
    "@angular/router": "^6.0.2",
    "@fortawesome/fontawesome-free": "^5.7.2",
    "@types/lodash": "^4.14.121",
    "angular-draggable-droppable": "^4.1.0",
    "core-js": "^2.5.4",
    "crypto-js": "^3.1.9-1",
    "dexie": "^2.0.4",
    "dexie-relationships": "^1.2.10",
    "document-register-element": "^1.7.2",
    "lodash": "^4.17.11",
    "mathjs": "^5.6.0",
    "ngx-electron": "^2.1.1",
    "pluralize": "^7.0.0",
    "rxjs": "^6.0.0",
    "sequelize": "^5.15.1",
    "sqlite3": "^4.0.6",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.3",
    "@angular/cli": "~6.0.3",
    "@angular/compiler-cli": "^7.2.6",
    "@angular/language-service": "^6.0.2",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "electron": "^4.1.1",
    "electron-builder": "^20.39.0",
    "electron-rebuild": "^1.8.4",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.4.2",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~3.1.1"
  }
}
